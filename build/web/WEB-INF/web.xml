<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>MeuPrimeiroServlet</servlet-name>
        <servlet-class>integrator.Enviar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Excluir</servlet-name>
        <servlet-class>integrator.Excluir</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Editar</servlet-name>
        <servlet-class>integrator.Editar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListarProdutos</servlet-name>
        <servlet-class>integrator.ListarProdutos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Enviar</servlet-name>
        <servlet-class>integrator.Enviar</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MeuPrimeiroServlet</servlet-name>
        <url-pattern>/MeuPrimeiroServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Excluir</servlet-name>
        <url-pattern>/Excluir</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Editar</servlet-name>
        <url-pattern>/Editar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListarProdutos</servlet-name>
        <url-pattern>/ListarProdutos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Enviar</servlet-name>
        <url-pattern>/Enviar</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
